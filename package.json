	"description": "The play area of Quantum Games that aim to grew up the intuition of Quantum Mechanics.",
	"keywords": [
		"education",
		"games"
	],
	"private": true,
	"icon": "static/favicon.ico",
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/u-sho/quantum-game-arena.git",
		"type": "git"
	},
	"bugs": {
